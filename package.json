{
  "name": "job-automation-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "dist-electron/electron-main.cjs",
  "homepage": "./",
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:electron\" \"wait-on http://localhost:5173 && electron .\"",
    "dev:renderer": "vite",
    "dev:electron": "tsup electron-main.ts preload.ts --format cjs --platform node --out-dir dist-electron --watch",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsup electron-main.ts preload.ts --format cjs --platform node --out-dir dist-electron",
    "build:renderer": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron-build": "npm run build && electron-builder"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "electron-is-dev": "^1.2.0",
    "imap": "^0.8.19",
    "mailparser": "^3.7.1",
    "nodemailer": "^6.9.7",
    "puppeteer": "^21.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/imap": "^0.8.40",
    "@types/mailparser": "^3.4.4",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@vitejs/plugin-react": "^4.1.1",
    "concurrently": "^8.2.2",
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "wait-on": "^7.2.0"
  }
}
